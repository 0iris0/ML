#DDL
CREATE DATABASE `sqlpractice`;
SHOW DATABASES;
DROP DATABASE `practice`;

USE `sqlpractice`;
CREATE TABLE `student`(
`student_id` INT PRIMARY KEY,  #AUTO_INCREMENT自動加1
`name` VARCHAR(20),  #可新增限制NOT NULL, UNIQUE, DEFAULT 'x'
`major` VARCHAR(20)
);
DESCRIBE `student`;
ALTER TABLE `student` ADD gpa DECIMAL(3,2);
ALTER TABLE `student` DROP COLUMN gpa;
DROP TABLE `student`;


#DML
SELECT*FROM `student`;
INSERT INTO `student` VALUES(1, '小白', '歷史');
INSERT INTO `student` VALUES(2, '小黑', '生物');
INSERT INTO `student` VALUES(3, '小綠', NULL);
INSERT INTO `student`(`major`,`student_id`) VALUES('英語',5);

